<?xml version="1.0" encoding="utf-8"?>
<resources>

  <style name="CAPButtonStyle" parent="android:Widget.ImageButton">
    <item name="android:minHeight">34dp</item>
    <item name="android:height">54dp</item>
    <item name="android:contentDescription">@string/cap_a11_button_label</item>
  </style>

  <style name="CAPButtonStyle.Light">
    <item name="android:src">@drawable/cap_logo_light</item>
    <item name="android:background">@drawable/cap_btn_background_light</item>
  </style>

  <style name="CAPButtonStyle.Dark">
    <item name="android:src">@drawable/cap_logo_dark</item>
    <item name="android:background">@drawable/cap_btn_background_dark</item>
  </style>
</resources>